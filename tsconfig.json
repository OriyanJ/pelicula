{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@app/*": ["app/*"],
      "@models": ["src/app/core/models"],
      "@services": ["src/app/core/services"],
      "@components": ["src/app/components"],
      "@bridge-interfaces": ["src/app/core/bridge/interfaces"],
      "@shared-components": ["src/app/shared/components"],
      "@shared-resolvers": ["src/app/shared/resolvers"],
      "@shared-directives": ["src/app/shared/directives"],
      "@shared-pipes": ["src/app/shared/pipes"],
      "@serializers": ["src/app/core/serializers"],
      "@state": ["src/app/core/state"],
      "@interceptors": ["src/app/core/interceptors"],
      "@globals": ["src/app/globals"]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
